{
   "action": {
      "default_icon": "img/icons/icon16.png",
      "default_title": "__MSG_ExtName__"
   },
   "background": {
      "service_worker": "background.js"
   },
   "content_scripts": [ {
      "all_frames": false,
      "js": [ "webstore.js" ],
      "matches": [ "https://chrome.google.com/webstore/*" ],
      "run_at": "document_start"
   } ],
   "default_locale": "en",
   "description": "__MSG_ExtShortDesc__",
   "differential_fingerprint": "",
   "externally_connectable": {
      "ids": [ "*" ],
      "matches": [ "*://*.avastbrowser.com/*", "*://*.avgbrowser.com/*", "*://*.ccleanerbrowser.com/*", "*://*.avast.com/*", "*://*.securebrowser.com/*" ]
   },
   "host_permissions": [ "*://*/*" ],
   "icons": {
      "128": "img/icons/icon128.png",
      "16": "img/icons/icon16.png",
      "48": "img/icons/icon48.png"
   },
   "incognito": "split",
   "key": "",
   "manifest_version": 3,
   "name": "__MSG_ExtName__",
   "permissions": [ "management", "tabs", "storage", "notifications", "unlimitedStorage", "webNavigation", "webRequest", "scripting", "nativeMessaging", "alarms" ],
   "version": "1.15.0.468",
   "web_accessible_resources": [ {
      "matches": [ "\u003Call_urls>" ],
      "resources": [ "popup.*", "img/logos/*" ]
   } ]
}
